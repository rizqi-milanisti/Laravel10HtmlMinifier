{
    "name": "rizqimilanisti/laravelminifier",
    "description": "Html Minifier adalah paket simpel untuk minify output Html, Css style, dan Javascript sebelum dirender ke browser untuk aplikasi Laravel anda.",
    "keywords": [
        "laravel",
        "Laravel Html Minifier",
        "Laravel-Html-Minifier",
        "Html Minify",
        "Minify"
    ],
    "authors": [
        {
            "name": "rizqimilanisti",
            "email": "muhammad.rizqi.1083@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/view": "^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "rizqimilanisti\\LaravelMinifier\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "rizqimilanisti\\LaravelMinifier\\MinifierServiceProvider"
            ]
        }
    }
}
